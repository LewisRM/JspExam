<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>PassFilter</filter-name>
  	<display-name>PassFilter</display-name>
  	<description></description>
  	<filter-class>org.lewis.filter.PassFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>PassFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <servlet>
       <servlet-name>CheckServlet</servlet-name>
       <servlet-class>org.lewis.servlet.CheckServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertFilm</servlet-name>
  	<display-name>InsertFilm</display-name>
  	<description></description>
  	<servlet-class>org.lewis.servlet.InsertFilm</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteServlet</servlet-name>
  	<display-name>DeleteServlet</display-name>
  	<description></description>
  	<servlet-class>org.lewis.servlet.DeleteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditServlet</servlet-name>
  	<display-name>EditServlet</display-name>
  	<description></description>
  	<servlet-class>org.lewis.servlet.EditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
       <servlet-name>CheckServlet</servlet-name>
       <url-pattern>/checkServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertFilm</servlet-name>
  	<url-pattern>/InsertFilm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServlet</servlet-name>
  	<url-pattern>/DeleteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditServlet</servlet-name>
  	<url-pattern>/EditServlet</url-pattern>
  </servlet-mapping>

</web-app>
